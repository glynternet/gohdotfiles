## VARIABLES

export-if-exists GOPATH "${HOME}/go"
source-environment go

source-environment git

source-organisation pocketmedia

export MUSICDATE="$(date "+20%y-%m_%B")"
export-if-exists MUSICDIR "$HOME/Music/$MUSICDATE"

export SERVER_HOST=http://localhost:8081
export-if-exists PG_MON_DATADIR "${HOME}/money/data"
export-if-exists PG_MON_WALARCHIVE_DIR "${HOME}/money/wal_archives"

export-if-exists DROPBOX $HOME/Dropbox
run-if-not-empty DROPBOX "source-environment dropbox"