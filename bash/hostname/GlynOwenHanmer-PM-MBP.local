## VARIABLES
export-if-exists GOPATH "${HOME}/go"
if [[ -n "$GOPATH" ]]; then
	export-if-exists GOP "${GOPATH}/src/github.com/Pocketbrain"
fi

## FUNCTIONS
pm-clone-go(){
	current="$(pwd)"
	cd ~/go/src/github.com/Pocketbrain/
	repoUrl="ssh://git@github.com/Pocketbrain/$1"
	git clone "$repoUrl"
	status=$?
	if [[ $status -ne 0 ]]; then
		return "Unable to clone repo at $repoUrl"
	fi
	cd "$current"
}
