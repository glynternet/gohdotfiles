export-if-exists GOPATH "${HOME}/go"
source-environment go

export-if-exists DROPBOX "${HOME}/Dropbox"
if [[ -n "$DROPBOX" ]]; then
	export-if-exists WIFICODES "${DROPBOX}/wifi_codes.txt"
	export-if-exists AWS_SIGNIN_KV "${DROPBOX}/scripts/bash/aws/aws_signin_url.kv"
	augment-path-if-exists "${DROPBOX}/scripts"
	augment-path-if-exists "${DROPBOX}/scripts/music"
fi
