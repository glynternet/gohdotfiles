export-if-exists DEVPATH "${HOME}/go"
export-if-exists GOPATH "${HOME}/go"
run-if-not-empty GOPATH source-environment go
run-if-not-empty GOPATH augment-cdpath-if-exists $GOPATH/src/bitbucket.org/glynny
run-if-not-empty GOPATH augment-cdpath-if-exists $GOPATH/src/github.com/glynternet

export-if-exists DROPBOX "$HOME/Dropbox" && \
	run-if-not-empty DROPBOX "source-environment dropbox"

source-environment git
generateGitBranchFunctions gh
source-environment dev

export-if-exists MUSIC "${HOME}/Music"
source-environment music

export-if-exists GITMOJIS_JSON "$HOME/tmp/gitmojis.json"

export-if-exists CYCLING_ROUTES_ARCHIVE "$DROPBOX/cycling/Routes"
GARMIN_MOUNT=/media/g/GARMIN
GARMIN_CARD=/media/g/3561-3563
source-environment garmin-strava
